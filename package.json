{
  "name": "image-optimization-api",
  "version": "0.1.0",
  "bin": {
    "image-optimization-api": "bin/index.js"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "husky",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "oxlint --tsconfig=tsconfig.json",
    "format": "prettier --write .",
    "deploy:transformer": "node --env-file=.env scripts/deploy.cjs",
    "deploy:uploader": "cdk deploy"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.18.0",
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@middy/core": "^6.1.6",
    "@middy/error-logger": "^6.1.6",
    "@middy/http-error-handler": "^6.1.6",
    "@middy/http-json-body-parser": "^6.1.6",
    "aws-cdk-lib": "^2.191.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.5.0",
    "http-errors": "^2.0.0",
    "lodash.merge": "^4.6.2",
    "mime-types": "^3.0.1",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/aws-lambda": "^8.10.149",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.14",
    "@types/lodash.merge": "^4.6.9",
    "@types/mime-types": "^2.1.4",
    "@types/node": "22.7.9",
    "aws-cdk": "^2.1012.0",
    "chalk": "^4.1.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "oxlint": "^0.16.7",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.6.3"
  }
